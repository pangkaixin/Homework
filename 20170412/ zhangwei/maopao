public class MaoPao{
	public static void main(String[] args){
		int[] a={1,8,9,2,0,7,3,5,4,6,10,13,12,11};
		for(int i=0;i<a.length;i++){
		int temp=0;
			for(int j=0;j<a.length-i-1;j++){
			if(a[j]<a[j+1]){
			temp=a[j];
			a[j]=a[j+1];
			a[j+1]=temp;
			}
			}
		}for(int b=0;b<a.length;b++){
			System.out.print(a[b]+",");
			}
	}